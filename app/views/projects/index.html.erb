<h2>List of projects</h2>

<% @projects.each do |project| %>
	<ul class="list-group">
	  <li class="list-group-item"><h3><%= project.name %></h3>
	  <%= link_to 'Show', project_path(project) %> | 
	  <%= link_to 'Edit', edit_project_path(project) %> | 
	  <%= link_to 'Delete', project_path(project), method: :delete, data: {confirm: 'Are you shure?'} %></li>
	</ul>
	
			
	 
<% end %>

